{
  "name": "functional-http-server",
  "version": "1.0.0",
  "description": "An example of a functional http server",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "immutable": "^3.8.1"
  },
  "scripts": {
    "test": "tap --cov test/**/*.js"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-plugin-fp": "^2.3.0",
    "tap": "^10.3.0",
    "tap-dot": "^1.0.5"
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "plugins": [
      "fp"
    ],
    "rules": {
      "fp/no-mutation": [
        "error",
        {
          "commonjs": true
        }
      ],
      "arrow-parens": "error"
    },
    "extends": "plugin:fp/recommended"
  }
}
